{
  "name": "newssite-database",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "studio": "drizzle-kit studio --config=drizzle.config.ts",
    "generate": "drizzle-kit generate --config=drizzle.config.ts",
    "migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "push": "drizzle-kit push --config=drizzle.config.ts",
    "seed": "tsx seed/seed.ts",
    "reset": "npm run generate && npm run push && tsx seed/reset.ts"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-switch": "^1.2.6",
    "@vercel/blob": "^2.0.0",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.7"
  }
}
